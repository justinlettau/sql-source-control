{
  "name": "sql-source-control",
  "version": "2.0.2",
  "description": "Simple CLI for getting SQL into source control systems.",
  "keywords": [
    "sql",
    "sql-server",
    "source-control",
    "git",
    "svn",
    "mercurial"
  ],
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "http://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/justinlettau/sql-source-control",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/sql-source-control"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/sql-source-control/issues"
  },
  "preferGlobal": true,
  "scripts": {
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json",
    "build": "tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "test": "nyc jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "format:all": "prettier --write **/*.{js,ts}"
  },
  "bin": {
    "ssc": "bin/ssc"
  },
  "files": [
    "bin/",
    "dist"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "checksum": "^0.1.1",
    "cli-table": "^0.3.1",
    "commander": "^2.19.0",
    "filenamify": "^2.1.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "inquirer": "^6.2.1",
    "mssql": "^4.3.0",
    "multimatch": "^3.0.0",
    "ora": "^3.0.0",
    "tedious": "^4.0.0",
    "ts-util-is": "^1.1.3",
    "update-notifier": "^2.5.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/checksum": "^0.1.30",
    "@types/cli-table": "^0.3.0",
    "@types/filenamify": "^2.0.1",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^0.0.43",
    "@types/jasmine": "^3.3.5",
    "@types/mock-fs": "^3.6.30",
    "@types/mssql": "^4.0.11",
    "@types/multimatch": "^2.1.3",
    "@types/ora": "^3.0.0",
    "@types/update-notifier": "^2.5.0",
    "@types/xml2js": "^0.4.3",
    "codecov": "^3.1.0",
    "husky": "^1.3.1",
    "jasmine": "^3.3.1",
    "mock-fs": "^4.7.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "standard-version": "^4.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-unicorn-science": "^1.3.0",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
