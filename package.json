{
  "name": "sql-source-control",
  "version": "2.1.1",
  "description": "Simple CLI for getting SQL into source control systems.",
  "keywords": [
    "sql",
    "sql-server",
    "source-control",
    "git",
    "svn",
    "mercurial"
  ],
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "https://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/justinlettau/sql-source-control",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/sql-source-control"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/sql-source-control/issues"
  },
  "preferGlobal": true,
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "build": "tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "test": "nyc jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "format": "prettier --write ."
  },
  "bin": {
    "ssc": "bin/ssc"
  },
  "files": [
    "bin/",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "checksum": "^0.1.1",
    "cli-table": "^0.3.1",
    "commander": "^6.1.0",
    "filenamify": "^4.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "inquirer": "^7.3.3",
    "mssql": "^6.2.2",
    "multimatch": "^4.0.0",
    "ora": "^5.1.0",
    "safe-buffer": "^5.2.1",
    "tedious": "^9.2.1",
    "ts-util-is": "^1.1.3",
    "update-notifier": "^4.1.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@justinlettau/renovate-config": "^0.1.2",
    "@types/checksum": "^0.1.31",
    "@types/cli-table": "^0.3.0",
    "@types/filenamify": "^2.0.2",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.3",
    "@types/inquirer": "^7.3.1",
    "@types/jasmine": "^3.5.14",
    "@types/mock-fs": "^4.10.0",
    "@types/mssql": "^6.0.4",
    "@types/multimatch": "^4.0.0",
    "@types/node": "^14.11.2",
    "@types/ora": "^3.2.0",
    "@types/update-notifier": "^4.1.1",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "codecov": "^3.7.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
    "jasmine": "^3.6.1",
    "mock-fs": "^4.13.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
