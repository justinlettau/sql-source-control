{
  "name": "sql-source-control",
  "version": "2.1.1",
  "description": "Simple CLI for getting SQL into source control systems.",
  "keywords": [
    "sql",
    "sql-server",
    "source-control",
    "git",
    "svn",
    "mercurial"
  ],
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "https://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/justinlettau/sql-source-control",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/sql-source-control"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/sql-source-control/issues"
  },
  "preferGlobal": true,
  "scripts": {
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:ts": "tslint -c ./tslint.json -p ./tsconfig.json",
    "lint:js": "eslint test/*.js -c .eslintrc",
    "build": "tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "test": "nyc jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "format:all": "prettier --write **/*.{js,ts}"
  },
  "bin": {
    "ssc": "bin/ssc"
  },
  "files": [
    "bin/",
    "dist"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "checksum": "^0.1.1",
    "cli-table": "^0.3.1",
    "commander": "^3.0.2",
    "filenamify": "^3.0.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "inquirer": "^7.3.1",
    "mssql": "^5.1.1",
    "multimatch": "^4.0.0",
    "ora": "^5.0.0",
    "tedious": "^6.7.0",
    "ts-util-is": "^1.1.3",
    "update-notifier": "^4.1.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@justinlettau/eslint-config": "^0.1.2",
    "@justinlettau/renovate-config": "^0.1.2",
    "@justinlettau/tslint-config": "^0.1.0",
    "@types/checksum": "^0.1.31",
    "@types/cli-table": "^0.3.0",
    "@types/filenamify": "^2.0.2",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.3",
    "@types/inquirer": "^6.5.0",
    "@types/jasmine": "^3.5.11",
    "@types/mock-fs": "^4.10.0",
    "@types/mssql": "^4.3.1",
    "@types/multimatch": "^4.0.0",
    "@types/node": "^12.12.50",
    "@types/ora": "^3.2.0",
    "@types/update-notifier": "^2.5.1",
    "@types/xml2js": "^0.4.5",
    "codecov": "^3.7.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.2.5",
    "jasmine": "^3.5.0",
    "mock-fs": "^4.12.0",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "standard-version": "^8.0.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
