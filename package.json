    "mssql": "4.0.4",
    "multimatch": "2.1.0",
    "tedious": "2.0.0",
    "ts-util-is": "1.1.2",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "@types/chalk": "0.4.31",
    "@types/commander": "2.9.1",
    "@types/deepmerge": "1.3.1",
    "@types/fs-extra": "4.0.0",
    "@types/glob": "5.0.30",
    "@types/inquirer": "0.0.35",
    "@types/mssql": "4.0.5",
    "@types/multimatch": "2.1.1",
    "@types/xml2js": "0.4.0",
    "standard-version": "4.2.0",
    "tslint": "5.5.0",
    "tslint-config-unicorn-science": "1.1.7",
    "typescript": "2.4.2"
  }
